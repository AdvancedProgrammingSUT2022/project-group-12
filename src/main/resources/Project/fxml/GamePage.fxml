<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.Pane?>
<Pane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="Project.Views.GameView">
    <MenuBar fx:id="menuBar" prefHeight="27.0" prefWidth="352.0">
        <menus>
            <Menu fx:id="cheat" text="Cheat Sheet">
                <MenuItem text="goto cheat sheet" onAction="#gotoCheatSelectPage"/>
            </Menu>
            <Menu fx:id="info" text="Info Panel">

            </Menu>
            <Menu onAction="#backToMenu" text="Back"/>
            <Menu onAction="#exit" text="Exit"/>

        </menus>
    </MenuBar>
    <ScrollPane fx:id="scrollPane" layoutX="-1.0" layoutY="25.0" prefHeight="374.0" prefWidth="600.0"
                style="-fx-background-color: transparent;">
        <Pane fx:id="tilePane">
            <Pane fx:id="buildingsPane" style="-fx-background-color: transparent;">
                <Pane fx:id="unitsPane" style="-fx-background-color: transparent;">

                </Pane>
            </Pane>
        </Pane>
    </ScrollPane>
    <Button fx:id="btn" layoutY="50" onMouseClicked="#click" prefHeight="24.0" prefWidth="242.0"/>
</Pane>
